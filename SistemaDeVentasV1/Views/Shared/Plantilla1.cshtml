<!DOCTYPE html>
<html>
<head>
    <title>Facturacion  | @ViewBag.Title </title>
    <!--  INICIO INCLUSION DE LIBRERIAS CSS DE LA PLANTILLA-->
    @RenderPage("~/Views/Shared/Tema1/Head.cshtml")
    <!--  FIN INCLUSION DE LIBRERIAS CSS DE LA PLANTILLA-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!--  INICIO header menu superior-->
        @RenderPage("~/Views/Shared/Tema1/Header.cshtml")
        <!--  FIN header menu superior-->
        <!--   Inicio Menu Izquierdo-->
        @RenderPage("~/Views/Shared/Tema1/LeftMenu.cshtml")
        <!--   Fin Menu Izquierdo-->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewBag.TituloControlador
                    <small>@ViewBag.InfoPagina</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="@Url.Action("Index","Home")"><i class="fa fa-dashboard"></i>INICIO </a></li>
                    <li><a href="#">@ViewBag.TituloControlador</a></li>
                    <li class="active">@ViewBag.Title</li>
                </ol>
            </section>
            <!-- Main content -->
            <section class="content">

                <!-- Default box -->
                <div class="box">
                    <div class="box-header with-border">
                        <center><h3 class="box-title">@ViewBag.TituloVentana</h3> </center>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                                <i class="fa fa-times"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                        @Html.Partial("_MsjError")
                        <div class="container col-md-12">
                            @RenderBody()

                        </div>

                    </div>
                    <!-- /.box-body -->

                    <div class="box-footer">
                        <center> <div class="text-bold">@ViewBag.FooterVentana</div> </center>
                    </div>
                    <!-- /.box-footer-->
                </div>
                <!-- /.box -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0
            </div>
            <strong>Copyright &copy; 2016-2017 <a href="#">Almsaeed Studio</a>.</strong> All rights
            reserved.
        </footer>
        <!-- Control Sidebar -->
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"> </div>
    </div>
    <!--Scripts del pie de pagina-->
    @RenderPage("~/Views/Shared/Tema1/FooterScripts.cshtml")

    @RenderSection("scripts", required: false)
    <!--FIN Scripts del pie de pagina-->
</body>
</html>

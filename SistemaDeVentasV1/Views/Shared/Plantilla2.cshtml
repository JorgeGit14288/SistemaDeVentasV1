<!DOCTYPE html>
<html>
<head>
    <title>Facturacion  | @ViewBag.Title </title>
    <!--  INICIO INCLUSION DE LIBRERIAS CSS DE LA PLANTILLA-->
    @RenderPage("~/Views/Shared/Tema1/Head.cshtml")
    <!--  FIN INCLUSION DE LIBRERIAS CSS DE LA PLANTILLA-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!--  INICIO header menu superior-->
        @RenderPage("~/Views/Shared/Tema1/Header.cshtml")
        <!--  FIN header menu superior-->
        <!--   Inicio Menu Izquierdo-->
        @RenderPage("~/Views/Shared/Tema1/LeftMenu.cshtml")
        <!--   Fin Menu Izquierdo-->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewBag.TituloControlador
                    <small>@ViewBag.InfoPagina</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="@Url.Action("Index","Home")"><i class="fa fa-dashboard"></i>INICIO </a></li>
                    <li><a href="#">@ViewBag.TituloControlador</a></li>
                    <li class="active">@ViewBag.Title</li>
                </ol>
            </section>
            <!-- Main content -->
            <section class="content">

                <div class="row col-md-12">
                    @Html.Partial("_MsjError")
                </div>
              
                <div class="container col-md-12">
                    @RenderBody()

                </div>
                <!-- /.box -->
            </section>
            <!-- /.content -->
            <footer class="main-footer">
                <div class="pull-right hidden-xs">
                    <b>Version</b> 2.3.12
                </div>
                <strong>Copyright &copy; 2014-2016 <a href="http://almsaeedstudio.com">Almsaeed Studio</a>.</strong> All rights
                reserved.
            </footer>
        </div>
        <!-- /.content-wrapper -->
        
        <!-- Control Sidebar -->
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"> </div>
    </div>
    <!--Scripts del pie de pagina-->
    @RenderPage("~/Views/Shared/Tema1/FooterScripts.cshtml")

    @RenderSection("scripts", required: false)
    <!--FIN Scripts del pie de pagina-->
</body>
</html>

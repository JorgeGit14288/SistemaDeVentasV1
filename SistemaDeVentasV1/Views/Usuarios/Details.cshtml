@model SistemaDeVentasV1.Models.AspNetUsers

@{
    ViewBag.Title = "Detalles de Usuario ";
    ViewBag.TituloControlador = "USUARIOS";
    ViewBag.InfoPagina = "Informacion Actual del usuario";
    ViewBag.TituloVentana = "Datos del Usuario";
    ViewBag.FooterVentana = "Si desea modificar la informacion del usuario seleccione la opcion editar";

    Layout = "~/Views/Shared/Plantilla1.cshtml";
}
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.UserName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.direccion)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.direccion)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.tel_casa)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tel_casa)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Activo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Activo)
        </dd>
        <dt>
            Roles
        </dt>
        <dd>
            @foreach (var r in Model.AspNetRoles)
            {
                @r.Name <br />
            }

        </dd>
    </dl>
</div>
<p>
   
    <input type="button" class="btn btn-info" value="Actualizar Registro" onclick="location.href='@Url.Action("Edit", "Usuarios", new {id = Model.Id })'" /> |
    <input type="button" class="btn btn-danger" value="Eliminar Registro" onclick="location.href='@Url.Action("Delete", "Usuarios", new {id = Model.Id})'" /> |
    <a href="@Url.Action("Index","Usuarios")" class="btn btn-success">Regresar a la lista</a>
</p>
